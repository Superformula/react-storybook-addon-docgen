{
  "name": "storybook-addon-docgen",
  "version": "4.0.0",
  "description": "A React Storybook addon to display docgen information.",
  "repository": {
    "type": "git",
    "url": "https://github.com/gongreg/react-storybook-addon-docgen.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "start-storybook -p 9010",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "build": "rm -rf ./dist && ./node_modules/.bin/babel --ignore tests,stories --plugins \"@babel/plugin-transform-runtime\" ./src --out-dir ./dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-info": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.9.0",
    "prop-types": "^15.6.2",
    "react": "^16.3.0",
    "react-dom": "^16.6.3"
  },
  "peerDependencies": {
    "@storybook/addons": "^5.0.0"
  },
  "dependencies": {
    "@emotion/styled": "^10.0.10",
    "github-markdown-css": "^2.4.1",
    "lodash.merge": "^4.6.1",
    "marked": "^0.3.6"
  },
  "main": "dist/index.js"
}
